        "-//GemStone Systems, Inc.//GemFire Declarative Caching 6.6//EN" "http://www.gemstone.com/dtd/cache6_6.dtd">

<client-cache>
    <pool name="idtcPool"  subscription-enabled="true" >
        <server host="localhost" port="7178" />
    </pool>

